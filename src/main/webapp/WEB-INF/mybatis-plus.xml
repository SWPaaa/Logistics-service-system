<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>



    <typeHandlers>
        <!-- 如果此处配置了映射, 则 BaseEnumTypeHandler 中就不需要 @MappedTypes 注解和 application.xml 中不需要设置属性 mybatis.type-handlers-package 值 -->
        <typeHandler handler="com.answer.aal.entity.BaseEnumTypeHandler" javaType="com.swp.Enum.UserTypeEnum" />
        <typeHandler handler="com.answer.xsf.entity.BaseEnumTypeHandler" javaType="com.swp.Enum.SexEnum" />
    </typeHandlers>

</configuration>
